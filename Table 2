create a table st_info with 3 subjects and calculate total and percentage 

create table st_info (PRN int, student_name varchar(20), m1 int, m2 int, m3 int, total int, percentage number);
insert into st_info values (101, 'abc', 88, 89, 77, NULL, NULL);
desc st_info; (table structure)
update st_info set total=m1+m2+m3;

insert into st_info values (101, 'abc', 88, 89, 77);

insert into st_info values (101, 'pqr', 65, 55, 59, NULL, NULL);
