insert into st_info values (101, 'xyz', 49, 58, 63, NULL, NULL);
insert into st_info values (101, 'ijk', 87, 78, 66, NULL, NULL);
insert into st_info values (101, 'sam', 79, 82, 83, NULL, NULL);
insert into st_info values (101, 'dan', 77, 87, 69, NULL, NULL);
insert into st_info values (101, 'uvw', 87, 79, 77, NULL, NULL);
insert into st_info values (101, 'jkl', 66, 68, 85, NULL, NULL);
insert into st_info values (101, 'mno', 77, 71, 72, NULL, NULL);

select * from st_info;

# to calculate total and percentag we use update command
update st_info set total=m1+m2+m3;
update st_info set percentage=total/3;

#to delete all rows and columns i.e date in the table
truncate table st_info 


#Table emp_info

